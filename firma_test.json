{
  "title": "V2_TEST",
  "templateVersion": "test",
  "argumentList": [
    "documentos",
    "urlBase",
    "urlDocumento",
    "fechaFirma"
  ],
  "data": {
    "signatureType": "PDF",
    "pdfSignatureConfig": {
      "VisibilityConfig": {
        "signaturePage": "last",
		"signatureLocation": "lower-left",
        "fontSize": 8,
        "font": "Courier"
      },
      "reason": "Firmado Digitalmente por {$SIGNER}, el ${fechaFirma}."
    },
    "certConfig": {
      "certificateLocation": ["MSCAPI","HWTOKEN","ASK"]
    },
    "fileOrigins": {
      "remote": {
        "URI": "${urlDocumento}/{$FILE_ID}",
        "method": "GET",
        "fileList": "${documentos}",
        "headers": {
          "Accept": "application/pdf"
        }
      }
    },
    "fileDestination": {
      "remote": {
        "URI": "${urlBase}/api/recibo/firmado",
        "method": "POST",
        "fileField": "archivo",
        "keepName": true,
        "formFields": {
          "firmado": true,
          "idArchivo": "{$FILE_ID}"
        }
      }
    },
    "interfaz": {
      "title": "Firmado de documentos Plantilla TEST",
      "hideDocuments": true
    },
    "template": {
      "id": "test_plantilla_01",
      "expiration": 1661262716733,
      "createdAt": 1659966716733
    }
  },
  "signature": ""
}
